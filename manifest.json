{
  "name": "Example 0: Hello World in Console",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "@konojunya",
  "description": "This is are example to print Hello World in Chrome console",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["background.js"],
    "css": ["style.css"]
  }],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "say hello world"
  }
}